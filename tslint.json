{
  "extends": "tslint:latest",
  "rules": {
    "array-type": [
        true,
        "array"
    ],
    "no-console": [
        true
    ],
    "curly": false,
    "one-line": [
        false
      ],
    "max-line-length": [
        120
    ],
    "object-literal-sort-keys": false,
    "variable-name": [
        true,
        "allow-leading-underscore"
    ]
  },
  "jsRules": {
  },
  "rulesDirectory": [
  ]
}